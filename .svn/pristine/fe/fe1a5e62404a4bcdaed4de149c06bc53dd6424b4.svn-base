<div class="iposs-windows_panel">
    <div class="panel-heading">
        <span class="panel-title">添加设备</span>
        <button type="button" class="close" aria-hidden="true">
            &times;
        </button>
    </div>
    <div class="panel-body">
        <form>
            <div class="">
                <div class="form-group">
                    <label class="iposs-windows_label">对象类型</label>
                    <div class="iposs-windows_label-for">
                        <select name="obj_type" class="form-control">
                            <option value="0">已知设备</option>
                            <option value="1">未知设备</option>
                        </select>
                    </div>
                </div>
                <div class="form-group vendor">
                    <label class="iposs-windows_label">厂商</label>
                    <div class="iposs-windows_label-for">
                        <select name="vendorId" class="form-control">
                        </select>
                    </div>
                </div>
                <div class="form-group modelList">
                    <label class="iposs-windows_label">设备型号</label>
                    <div class="iposs-windows_label-for">
                        <select name="serial" class="form-control">
                        </select>
                    </div>
                </div>
                <div class="form-group resList">
                    <label class="iposs-windows_label">资源类型</label>
                    <div class="iposs-windows_label-for">
                        <select name="resourceTypeId" class="form-control">
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <label class="iposs-windows_label">IP地址</label>
                    <div class="iposs-windows_label-for">
                        <input class="form-control" type="text" name="ip" required="true">
                    </div>
                </div>
            </div>

            <div class="">
                <div class="form-group">
                    <label class="iposs-windows_label">名称</label>
                    <div class="iposs-windows_label-for">
                        <input class="form-control" type="text" name="name" required="true">
                    </div>
                </div>
                <div class="form-group">
                    <label class="iposs-windows_label">读口令</label>
                    <div class="iposs-windows_label-for">
                        <input class="form-control" type="password" name="readcommunity" required="true">
                    </div>
                </div>
                <div class="form-group">
                    <label class="iposs-windows_label">写口令</label>
                    <div class="iposs-windows_label-for">
                        <input class="form-control" type="password" name="writecommunity" required="true">
                    </div>
                </div>
                <div class="form-group">
                    <label class="iposs-windows_label">采集机</label>
                    <div class="iposs-windows_label-for">
                        <select name="gatherId" class="form-control">
                        </select>
                    </div>
                </div>
            </div>

            <button type="submit" class="qtopo-btn iposs-windows_submit-btn">确认</button>
        </form>
    </div>
</div>